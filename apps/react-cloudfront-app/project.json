{
  "name": "react-cloudfront-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "development",
      "options": {
        "outputPath": "apps/react-cloudfront-app/dist"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-cloudfront-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-cloudfront-app:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "react-cloudfront-app:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-cloudfront-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-cloudfront-app:build:development"
        },
        "production": {
          "buildTarget": "react-cloudfront-app:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": [
        "coverage/apps/react-cloudfront-app"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/react-cloudfront-app"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/react-cloudfront-app/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/react-cloudfront-app",
        "color": true,
        "command": "sls client deploy --no-config-change --no-policy-change --no-cors-change --no-confirm"
      }
    },
    "invalidateCloudFrontCache": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/react-cloudfront-app",
        "color": true,
        "command": "sls invalidateCloudFrontCache"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/react-cloudfront-app",
        "color": true,
        "commands": [
          "sls client deploy --no-config-change --no-policy-change --no-cors-change --no-confirm",
          "sls invalidateCloudFrontCache"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}
